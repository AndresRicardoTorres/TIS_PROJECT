<h1>Listing user_histories</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Actors</th>
    <th>Dependence</th>
    <th>Priotity</th>
    <th>Iteration</th>
    <th>True time</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_histories.each do |user_history| %>
  <tr>
    <td><%= user_history.name %></td>
    <td><%= user_history.actors %></td>
    <td><%= user_history.dependence %></td>
    <td><%= user_history.priotity %></td>
    <td><%= user_history.iteration %></td>
    <td><%= user_history.true_time %></td>
    <td><%= link_to 'Show', project_user_history_path(@project, user_history) %></td>
    <td><%= link_to 'Edit', edit_project_user_history_path(@project, user_history) %></td>
    <td><%= link_to 'Destroy',  project_user_history_path(@project, user_history), confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User history', new_project_user_history_path(@project) %>
<%= link_to 'Back to Project', @project %>
